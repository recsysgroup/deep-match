{
  "embedding_size": 64,
  "train_batch_size": 128,
  "train_neg_size": 128,
  "eval_point_size": 10000,
  "eval_pos_size": 10000,
  "eval_neg_size": 3883,
  "columns": [
    {
      "name": "user:user_id",
      "type": "single"
    },
    {
      "name": "user:gender",
      "type": "single"
    },
    {
      "name": "user:age",
      "type": "single"
    },
    {
      "name": "user:occupation",
      "type": "single"
    },
    {
      "name": "user:zip_code",
      "type": "single"
    },
    {
      "name": "user:watched_id_seq",
      "type": "seq",
      "seq_len": 10
    },
    {
      "name": "item:item_id",
      "type": "single"
    }
  ],
  "loss": {
    "name": "neg_bpr",
    "l2": 0.0001
  },
  "aggregators": {
    "user": {
      "name": "self_attention",
      "l2": 1e-6
    },
    "item": {
      "name": "sum"
    }
  },
  "evals": [
    {
      "name": "auc"
    },
    {
      "name": "hr",
      "topk": [
        10,
        50,
        100
      ]
    },
    {
      "name": "map",
      "topk": [
        10,
        50,
        100
      ]
    }
  ],
  "user": [
    {
      "name": "user_id",
      "layers": [
        {
          "type": "hash",
          "size": 100000
        },
        {
          "type": "embedding",
          "name": "user_emb",
          "size": 100000
        }
      ]
    },
    {
      "name": "gender",
      "layers": [
        {
          "type": "hash",
          "size": 100
        },
        {
          "type": "embedding",
          "name": "gender_emb",
          "size": 100
        }
      ]
    },
    {
      "name": "age",
      "layers": [
        {
          "type": "hash",
          "size": 1000
        },
        {
          "type": "embedding",
          "name": "age_emb",
          "size": 1000
        }
      ]
    },
    {
      "name": "occupation",
      "layers": [
        {
          "type": "hash",
          "size": 10000
        },
        {
          "type": "embedding",
          "name": "occupation_emb",
          "size": 10000
        }
      ]
    },
    {
      "name": "zip_code",
      "layers": [
        {
          "type": "hash",
          "size": 10000
        },
        {
          "type": "embedding",
          "name": "zip_code_emb",
          "size": 10000
        }
      ]
    },
    {
      "name": "watched_id_seq",
      "layers": [
        {
          "type": "hash",
          "size": 1000000
        },
        {
          "type": "embedding",
          "name": "item_emb",
          "size": 1000000
        },
        {
          "type": "reduce_mean"
        }
      ]
    }
  ],
  "item": [
    {
      "name": "item_id",
      "type": "single",
      "layers": [
        {
          "type": "hash",
          "size": 1000000
        },
        {
          "type": "embedding",
          "name": "item_emb",
          "size": 1000000
        }
      ]
    }
  ]
}
